<%- include('./partials/header') %> <%- include('./partials/navbar') %>
<div
  class="container-fluid d-flex col-md-8 col-lg-3 pt-md-5 flex-column overflow-auto h-100 justify-content-start my-5 pb-4"
>
  <div class="card">
    <div
      class="card-body overflow-auto text-body my-1 py-1 bg-light bg-gradient"
    >
      <h5 class="text-center">Search Results</h5>
      <div class="d-flex flex-row justify-content-between align-items-center">
        <% users.forEach(user => { %>
        <img
          src="/<%= user.avatar %>"
          alt=""
          style="width: 50px; height: auto; border-radius: 25px"
        />
        <%= user.first_name %> <%= user.last_name %> <% }) %>
      </div>
    </div>
  </div>
</div>

<%- include('./partials/footer') %>
